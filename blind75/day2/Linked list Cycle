// Linked List
//fast and slow pointers

class Solution {
public:
    bool hasCycle(ListNode *head) {
        if(!head)
            return false;
        ListNode *fast=head;
        ListNode *slow=head;
        while(fast!=NULL and fast->next != NULL){
            fast=fast->next->next;
            slow=slow->next;
            
            if(fast==slow)
                return true;
        }
        return false;
        
    }
};
